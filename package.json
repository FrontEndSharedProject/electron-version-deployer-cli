{
  "name": "electron-version-deployer-cli",
  "private": false,
  "version": "0.0.12",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "scripts": {
    "serve": "esno ./watch.ts",
    "build": "MODE=production esno ./watch.ts && tsc -p ./tsconfig.dist.json -emitDeclarationOnly && tsc-alias -p ./tsconfig.dist.json",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,tsx,jsx,json,html}\"",
    "typecheck": "tsc -p tsconfig.json"
  },
  "bin": {
    "evd": "./dist/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/archiver": "^5.3.2",
    "@types/node": "^20.2.5",
    "electron": "^25.0.1",
    "prettier": "^2.8.8",
    "tsc-alias": "^1.8.6",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "@inquirer/prompts": "^1.2.3",
    "changelog-parser": "^3.0.1",
    "commander": "^10.0.1",
    "dompurify": "^3.0.3",
    "download": "^8.0.0",
    "esno": "^0.16.3",
    "jsdom": "^22.1.0",
    "log-symbols": "=4.1.0",
    "marked": "^5.0.4",
    "netlify-cli": "^15.2.0"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "extract-zip": "^2.0.1",
    "pkg-up": "3.1.0"
  },
  "peerDependencies": {
    "electron": ">=10.0.0",
    "@inquirer/prompts": "^1.2.3",
    "changelog-parser": "^3.0.1",
    "commander": "^10.0.1",
    "dompurify": "^3.0.3",
    "download": "^8.0.0",
    "esno": "^0.16.3",
    "jsdom": "^22.1.0",
    "log-symbols": "=4.1.0",
    "marked": "^5.0.4",
    "netlify-cli": "^15.2.0",
    "vite": "^4.3.9"
  }
}
